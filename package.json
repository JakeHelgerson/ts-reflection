{
  "name": "ts-type-checked",
  "version": "0.1.2",
  "description": "Type checking utilities for TypeScript.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf transformer/**/*.{js,d.ts}",
    "lint": "eslint . --fix --ext .ts",
    "prepublish": "yarn build",
    "test:examples": "cd examples && yarn && yarn install:local && yarn build && yarn start",
    "test:version": "./scripts/test.sh",
    "test:versions": "./scripts/test-all-versions.sh",
    "test": "yarn test:versions && yarn test:examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janjakubnanista/ts-type-checked.git"
  },
  "keywords": [
    "TypeScript",
    "type checking",
    "type checks",
    "duck typing",
    "type guards"
  ],
  "author": "Jan Jakub Nanista <jan.jakub.nanista@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/janjakubnanista/ts-type-checked/issues"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "transformer/**/*.js",
    "transformer/**/*.d.ts"
  ],
  "homepage": "https://github.com/janjakubnanista/ts-type-checked#readme",
  "devDependencies": {
    "@types/node": "^13.13.5",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "fast-check": "^1.24.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "tslib": "^1.11.2",
    "ttypescript": "^1.5.10",
    "typescript": "^3.7.2"
  },
  "peerDependencies": {
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}(x)?": [
      "eslint --fix"
    ]
  }
}
